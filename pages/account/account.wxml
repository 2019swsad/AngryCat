<!--pages/account/account.wxml-->
<view class="personal_info">
	<view class="photo_wrap">
		<image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" ></image>
	</view>
	<view class="nickname">
		{{userInfo.nickName}}
	</view>
</view>
<view class="wode_item_wrap">
	<navigator class="wode_item" wx:for="{{mode}}" url="">
		{{item}}
	</navigator>
</view>