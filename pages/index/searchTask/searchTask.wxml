<!--pages/index/searchTask/searchTask.wxml-->

<import src="../../../template/singleTask.wxml" />

<view>
  <view class='search_box'>
    <text>请输入关键字</text>
    <input placeholder='关键字'></input>
    <button bindtap='execSearch'>搜索</button>
  </view>

  <view class='result_tasks'>

    <block wx:for="{{resultTasks}}" wx:for-item="task">
      <template is="singleTask" data="{{...task}}"></template>
      <!-- <text class="log-item">{{index + 1}}. {{log}}</text> -->
    </block>

    <!-- <text>{{stringifyRes}}</text> -->

  </view>

</view>