/* regist/regist.wxss */
page {
  width: 100%;
  height: 100%;
  background-color: white;

}
.main {
  display: flex;
  flex-direction: column;
  justify-content:flex-start;
  align-items: center;
}

.sign {
  padding-top: 40px;
  color: #2eada7;
  font-family: 'Ubuntu', sans-serif;
  font-weight: bold;
  font-size: 23px;
}

.register_input {
  width: 270px;
  color: rgb(38, 50, 56);
  height: 50px;
  font-weight: 500;
  font-size: 18px;
  letter-spacing: 1px;
  border: none;
  outline: none;
  box-sizing: border-box;
  border-bottom: 1px solid rgb(235, 235, 235);
  text-align: left;
  margin-bottom: 20px;
  font-family: 'Ubuntu', sans-serif;
}


form.form1 {
  padding-top: 30px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}


.input_style {
  color: rgb(195,197,198);
}

.submit_btn {
  width: 150px;
  margin-top: 50px;
  font-weight: 700;
}
    